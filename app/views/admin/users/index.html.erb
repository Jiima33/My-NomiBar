<h5>ユーザー一覧</h5>
<% @users.each do |user| %>
  <%= image_tag user.get_profile_image(20,20) %>
  ID<%= user.id %>
  ユーザー名<%= user.name %>
  自己紹介文<%= user.introduction %>
  メールアドレス<%= user.email %>
  アカウント状況
  <% if user.is_stopped == true %>
  停止
  <% else %>
  有効
  <% end %>
  <%= link_to "詳細", admin_user_path(user.id) %>
<% end %>

